<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet  id="1" author="Konofeev Roman">
        <insert tableName="CONTENT">
            <column name = "id"      valueNumeric = "1"/>
            <column name = "name"    value        = "Тестовая статья"/>
            <column name = "content" value        = "Содержимое статьи"/>
        </insert>
        <insert tableName = "CONTENT">
            <column name = "id"      valueNumeric = "2"/>
            <column name = "name"    value        = "Ещё одна статья"/>
            <column name = "content" value        = "Содержимое второй статьи"/>
        </insert>
        <insert tableName = "CONTENT">
            <column name = "id"      valueNumeric = "3"/>
            <column name = "name"    value        = "Другая статья для сайта"/>
            <column name = "content" value        = "Тестовая статья для сайта"/>
        </insert>
    </changeSet>

</databaseChangeLog>
